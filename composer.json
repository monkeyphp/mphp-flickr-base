{
    "name": "monkeyphp/mphp-flickr-base",
    "description": "Base package for the Flickr library created by Monkeyphp.",
    "type": "library",
    "homepage": "https://github.com/monkeyphp/mphp-flickr-base",
    "license": "MIT",
    "keywords": [
        "Flickr",
        "photos"
    ],
    "authors": [
        {
            "name": "David White",
            "email": "git@monkeyphp.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "git@monkeyphp.com"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name":"apigen/apigen",
                "version": "2.8.0",
                "dist": {
                    "url": "https://github.com/downloads/apigen/apigen/ApiGen-2.8.0-standalone.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "zendframework/zend-http": "2.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.14",
        "apigen/apigen": "*"
    },
    "autoload": {
        "psr-0": {
            "MphpFlickrBase": "src/"
        }
    },
    "version": "1.0.0"
}
